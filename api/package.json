{
  "name": "api",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test-reload": "nodemon --config nodemon.test.json",
    "production": "node --experimental-fetch --loader ts-node/esm -r dotenv/config src/index.ts",
    "codegen": "concurrently --kill-others \"node --experimental-fetch --loader ts-node/esm -r dotenv/config codegen.ts\" \"ts-node codegen-waiton.ts && graphql-codegen --config codegen.yml && get-graphql-schema http://localhost:4001 > src/generated/schema.gql\"",
    "start": "nodemon --config nodemon.json"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-mongodb": "^2.4.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.8",
    "@types/ws": "^8.5.3",
    "apollo-server": "^3.10.1",
    "babel-jest": "^29.0.1",
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "jest": "^29.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.9.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1",
    "ws": "^8.8.1"
  }
}
